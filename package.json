{
  "name": "swr-openapi",
  "version": "2.1.0",
  "description": "Generate SWR hooks from OpenAPI schemas",
  "license": "MIT",
  "repository": "htunnicliff/swr-openapi",
  "funding": "https://github.com/sponsors/htunnicliff",
  "author": {
    "name": "Hunter Tunnicliff"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "swr",
    "openapi",
    "rest",
    "generator",
    "client",
    "fetch"
  ],
  "scripts": {
    "prepublish": "npm run build && npm run types:check && npm run format:check",
    "build": "tsup",
    "types:check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "devDependencies": {
    "@types/node": "20.11.19",
    "openapi-fetch": "0.9.2",
    "openapi-typescript": "6.7.4",
    "openapi-typescript-helpers": "0.0.7",
    "prettier": "3.2.5",
    "swr": "2.2.5",
    "tsup": "7.2.0",
    "typescript": "5.2.2"
  },
  "peerDependencies": {
    "openapi-fetch": ">=0.9",
    "openapi-typescript": "6",
    "swr": "2",
    "typescript": "5"
  }
}
