{
  "name": "swr-openapi",
  "version": "0.5.1",
  "description": "Generate SWR hooks from OpenAPI schemas",
  "license": "MIT",
  "repository": "htunnicliff/swr-openapi",
  "funding": "https://github.com/sponsors/htunnicliff",
  "author": {
    "name": "Hunter Tunnicliff",
    "email": "hunter@tunnicliff.co"
  },
  "main": "./dist/index.js",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "swr",
    "openapi",
    "rest",
    "generator",
    "client",
    "fetch"
  ],
  "scripts": {
    "postinstall": "patch-package",
    "build": "del ./dist && tsc",
    "types:check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "openapi-typescript": "^5.4.0",
    "openapi-typescript-fetch": "^1.1.3",
    "patch-package": "^6.4.7",
    "swr": "^2.0.0-beta.3"
  },
  "devDependencies": {
    "@types/node": "^16",
    "@types/react": "^18.0.11",
    "del-cli": "^4.0.1",
    "prettier": "^2.6.2",
    "typescript": "^4.7.2"
  },
  "peerDependencies": {
    "openapi-typescript": "^5.0.0",
    "swr": "^2.0.0-beta.3",
    "typescript": ">=4.0.0"
  },
  "volta": {
    "node": "16.15.1"
  }
}
